{
    "name": "langdev-2025-demo",
    "version": "2.0.0",
    "license": "MIT",
    "type": "module",
    "scripts": {
        "again": "npm run build && npm run styles && npm run dev",
        "initialize": "npm run clean && npm install && npm run build",
        "clean": "rm -rf node_modules/ public/build/dist/ package.lock.json",
        "build": "npm run generate && tsc",
        "build-app": "vite build",
        "dev": "vite",
        "generate": "freon all -d src/defs -o src/freon",
        "preview": "vite preview",
        "to-lionweb": "freon lionweb-it -d src/defs -o src/freon",
        "start": "sirv public --port 8000",
        "validate": "svelte-check --tsconfig ./tsconfig.json",
        "errors-only": "svelte-check --threshold error --tsconfig ./tsconfig.json",
        "styles": "cross-env NODE_ENV=production npx @tailwindcss/cli -i styles/webapp-styles.css -o ./public/site.css",
        "format": "prettier --write .",
        "server": "node ./node_modules/@freon4dsl/server/dist/server/server-starter.js"
    },
    "devDependencies": {
        "@internationalized/date": "^3.9.0",
        "@lucide/svelte": "^0.544.0",
        "@sveltejs/vite-plugin-svelte": "^5.1.0",
        "@tailwindcss/cli": "^4.1.10",
        "@tailwindcss/vite": "^4.1.10",
        "@tanstack/table-core": "^8.21.3",
        "@tsconfig/svelte": "^5.0.4",
        "bits-ui": "^2.11.4",
        "@sveltejs/adapter-auto": "^6.1.0",
        "cross-env": "7.0.3",
        "svelte-check": "^4.1.5",
        "svelte-preprocess": "^6.0.3",
        "tailwind-variants": "^3.1.1",
        "tailwindcss": "^4.1.4",
        "typescript": "^5.8.2",
        "vite": "^6.3.5"
    },
    "dependencies": {
        "@freon4dsl/core": "2.0.0",
        "@freon4dsl/core-svelte": "2.0.0",
        "@freon4dsl/meta": "2.0.0",
        "@freon4dsl/server": "2.0.0",
        "@freon4dsl/weblib-flowbite": "2.0.0",
        "@lucide/svelte": "0.544.0",
        "@types/node": "^20.14.1",
        "kotlin": "^1.5.21",
        "lodash": "^4.17.21",
        "mobx": "^6.12.3",
        "mobx-utils": "^6.0.8",
        "phosphor-svelte": "^3.0.1",
        "sirv-cli": "^2.0.2",
        "tslib": "^2.6.2"
    }
}
